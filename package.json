{
  "name": "multiselect-combo-box",
  "version": "1.0.1",
  "description": "A multi select combo box web component based on Polymer 2.x and the vaadin-combo-box",
  "main": "multiselect-combo-box.html",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "bower i && npm run lint && wct",
    "lint": "npm-run-all --parallel lint:*",
    "lint:css": "stylelint *.html src/*.html demo/*.html theme/**/*.html test/*html",
    "lint:html": "eslint *.html src demo test --ext .html",
    "lint:js": "eslint *.js",
    "lint:polymer": "polymer lint --input ./src/*.html ./theme/**/*.html",
    "start": "npm run lint && wct && polymer serve --open"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatanaso/multiselect-combo-box"
  },
  "keywords": [
    "multiselect-combo-box",
    "web-components",
    "polymer"
  ],
  "author": "Goran",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/gatanaso/multiselect-combo-box/issues"
  },
  "devDependencies": {
    "eslint": "^5.9.0",
    "eslint-config-vaadin": "^0.2.7",
    "eslint-plugin-html": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "stylelint": "^9.8.0",
    "stylelint-config-vaadin": "^0.1.4",
    "wct-istanbul": "^0.14.3",
    "web-component-tester": "^6.9.0"
  }
}
