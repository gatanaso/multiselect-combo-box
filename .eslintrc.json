{
  "extends": "vaadin",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "plugins": [
    "html"
  ],
  "globals": {
    "Polymer": false,
    "Vaadin": false,
    "MultiselectComboBoxMixin": false
  },
  "rules": {
    "quotes": [
      "error",
      "single",
      {
        "avoidEscape": true
      }
    ]
  }
}
